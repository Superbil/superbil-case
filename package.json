{
  "name": "superbil.co",
  "version": "0.1.1",
  "private": true,
  "homepage": "https://case.superbil.co",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "react-scroll": "^1.7.12"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/scss -o src/style",
    "watch-css": "yarn run build-css && node-sass-chokidar src/scss -o src/style --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "yarn run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "yarn build && cp CNAME build/",
    "deploy": "gh-pages -b master -d build -r git@github.com:Superbil/superbil-case.git -m 'Deploy Pages'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.5"
  }
}
